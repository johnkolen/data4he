<% content = render @object, 
turbo: @turbo %>
<%= ov_modal_objects_view @klass, content, @turbo do %>
<% content_for :title, "#{ @klass_str }>" %>

<h1><%= @klass_str %></h1>

<%= content %>

<br>

<div class='ov-button-row'>
<%= ov_edit @object %>
<%= ov_index @klass %>
<%= ov_delete @object %>
</div>
<% end %>
